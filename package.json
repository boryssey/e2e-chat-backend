{
  "name": "e2e-chat-backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "engine": {
    "node": "20.11.0"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register test/**/*.ts",
    "start": "fastify start -l info dist/app.js",
    "build": "tsc",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "print-routes": "pnpm run build && fastify print-routes --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate"
  },
  "xo": {
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "capitalized-comments": "off",
      "import/extensions": "off",
      "@typescript-eslint/consistent-type-definitions": "off",
      "@typescript-eslint/naming-convention": "off",
      "unicorn/filename-case": "off",
      "n/prefer-global/process": "off",
      "unicorn/prefer-module": "off",
      "unused-imports/no-unused-imports": "error",
      "no-mixed-spaces-and-tabs": 0,
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ],
      "new-cap": "off",
      "no-use-extend-native/no-use-extend-native": "off",
      "unicorn/no-array-reduce": "off"
    },
    "ignore": [
      "scripts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^4.6.1",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/sensible": "^5.6.0",
    "@fastify/type-provider-typebox": "^4.1.0",
    "@sinclair/typebox": "^0.32.20",
    "ajv": "^8.13.0",
    "ajv-formats": "^3.0.1",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.28.1",
    "fastify-cli": "^6.3.0",
    "fastify-plugin": "^4.5.1",
    "fastify-socket.io": "^5.0.0",
    "fluent-json-schema": "^4.2.1",
    "npm": "^10.5.2",
    "postgres": "^3.4.4",
    "socket.io": "^4.7.5",
    "typescript": "^5.5.4",
    "uuid-readable": "^0.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.4.4",
    "@types/uuid-readable": "^0.0.3",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.24.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "fastify-tsconfig": "^2.0.0",
    "pino-pretty": "^11.0.0",
    "ts-node": "^10.4.0",
    "xo": "^0.58.0"
  }
}